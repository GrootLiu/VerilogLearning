    %assign/vec4 v0x5601adfc05a0_0, 0;
    %pushi/vec4 512, 0, 10;
    %assign/vec4 v0x5601adfc0680_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5601adfc0420_0, 0;
