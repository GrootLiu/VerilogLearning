    %assign/vec4/off/d v0x5601adfc05a0_0, 4, 5;
    %load/vec4 v0x5601adfc0680_0;
    %parti/s 9, 1, 2;
    %ix/load 4, 0, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x5601adfc0680_0, 4, 5;
    %pushi/vec4 0, 0, 13;
    %assign/vec4 v0x5601adfc0170_0, 0;
    %jmp T_0.8;
T_0.7 ;
    %load/vec4 v0x5601adfc0170_0;
    %addi 1, 0, 13;
    %assign/vec4 v0x5601adfc0170_0, 0;
T_0.8 ;
    %load/vec4 v0x5601adfc0680_0;
    %parti/s 1, 0, 2;
