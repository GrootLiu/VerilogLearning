    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5601adfc0420_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5601adfc0760_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 1;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 1;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %jmp T_0.4;
T_0.2 ;
    %load/vec4 v0x5601adfc0310_0;
