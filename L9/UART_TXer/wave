    %assign/vec4 v0x5601adfc0760_0, 0;
T_0.5 ;
    %jmp T_0.4;
T_0.3 ;
    %load/vec4 v0x5601adfc0170_0;
    %pad/u 32;
    %cmpi/e 4999, 0, 32;
    %jmp/0xz  T_0.7, 4;
    %load/vec4 v0x5601adfc05a0_0;
    %parti/s 9, 1, 2;
    %ix/load 4, 0, 0;
